{
    "cwd": "${fileDirname}",
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-i",
                "example.txt",
                "-2",
                 "-b"
            ],
            "cwd": "${fileDirname}",
            "env": {
                "PYTHONDEBUGPYLOGLEVEL": "ERROR"
            }
        }
    ]
}
